<?xml version="1.0" encoding="UTF-8"?>
<component type="desktop-application">
    <id>com.codersnext.cadviewer</id>
    <metadata_license>MIT</metadata_license>
    <project_license>MIT</project_license>
    <name>CADViewer</name>
    <summary>An app for viewing CAD</summary>
    <description>
        <p>  Intended for use with command-line based CAD programs such as CSharpCAD, pycscad and jscad.
        </p>
    </description>
    <launchable type="desktop-id">com.codersnext.cadviewer.desktop</launchable>
    <url type="homepage">https://github.com/briansturgill/CSharpCAD</url>
    <provides>
        <binary>python{{ python-version }}</binary>
    </provides>
</component>
